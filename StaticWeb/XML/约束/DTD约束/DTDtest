<?xml version ="1.0" encoding = 'utf-8' ?>
<!--本地引入-->
<!DOCTYPE students SYSTEM "student.dtd">
<students>
    <student number="attribute">
        <!-- 缺陷：无法对属性的内容进行约束 -->
        <name>yuye</name>
        <age>10000</age>
        <sex>female</sex>
    </student>
   <!--  属性number设置为不可重复
   <student number="attribute">
        &lt;!&ndash; 缺陷：无法对属性的内容进行约束 &ndash;&gt;
        <name>yuye</name>
        <age>10000</age>
        <sex>female</sex>
    </student>
    -->

</students>